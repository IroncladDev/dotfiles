  apps (macro M-spc)
  prompt (macro M-A-spc)
  emojis (macro M-S-spc)
  color-picker (macro M-prnt)

  screenshot-region (macro prnt)
  screenshot-window (macro S-prnt)
  screenshot-display (macro C-prnt)

  screen-record-region (macro A-prnt)
  screen-record-region-audio (macro A-S-prnt)
  screen-record-display (macro C-A-prnt)
  screen-record-display-audio (macro C-A-S-prnt)
)
