(defseq
  ;; editing
  s-home (n)
  s-end (o)

  apps (f f)
  prompt (f p)
  emojis (f e)
  color-picker (f c)

  screenshot-region (s s)
  screenshot-window (s w)
  screenshot-display (s d)

  screen-record-region (r r)
  screen-record-display (r d)
