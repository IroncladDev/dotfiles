(defalias
  cma (tap-dance $tap-time (
    (macro a)
    (macro M-a)
  ))
  cmz (tap-dance $tap-time (
    (macro z)
    (macro M-z)
  ))
  cmx (tap-dance $tap-time (
    (macro x)
    (macro M-x)
  ))
  cmc (tap-dance $tap-time (
    (macro c)
    (macro M-c)
  ))
  cmv (tap-dance $tap-time (
    (macro v)
    (macro M-v)
  ))
)
